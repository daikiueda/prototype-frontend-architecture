{
  "name": "apps-ui-framework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "webpack-dev-server --config ./webpack.config.dev.babel.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "webpack --config ./webpack.config.prod.babel.js --progress",
    "build:storybook": "build-storybook -c .storybook -o ./dist/storybook",
    "build-all": "run-p build build:storybook",
    "lint": "run-s lint:*",
    "lint:js": "eslint \"{src,test}/**/*.js\"",
    "test": "run-s test:logics",
    "test:logics": "cross-env NODE_PATH=src/ ava",
    "test-cov:logics": "nyc npm run test:logics",
    "depcruise": "depcruise --exclude \"(^node_modules|.stories|.scss$|.css$)\" --output-type dot ./src | dot -T svg > dependency-graph.svg",
    "gh-pages": "gh-pages -d ./dist",
    "deploy": "run-s build-all gh-pages"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "normalize.css": "^7.0.0",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-transition-group": "^1.2.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-form": "^6.8.0",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.1.5",
    "@storybook/react": "^3.1.5",
    "autoprefixer": "^7.1.1",
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "dependency-cruiser": "^2.2.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.2",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "ava": {
    "files": [
      "test/ava/**/*.test.js"
    ],
    "source": "./src/**/*.js",
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
