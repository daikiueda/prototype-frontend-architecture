{
  "name": "apps-ui-framework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "webpack-dev-server --config ./webpack.config.dev.babel.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "webpack --config ./webpack.config.prod.babel.js -p",
    "build:storybook": "build-storybook -c .storybook -o ./dist/storybook",
    "build-all": "run-p build build:storybook",
    "lint": "run-s lint:*",
    "lint:js": "eslint \"{src,test}/**\"",
    "test": "run-s test:logics",
    "test:logics": "ava",
    "test:logics-cov": "nyc --reporter=lcov --reporter=text-summary npm run test-logics",
    "gh-pages": "gh-pages -d ./dist",
    "deploy": "run-s build-all gh-pages"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-transition-group": "^1.1.3",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.0.0-alpha.0",
    "@storybook/react": "^3.0.0-alpha.0",
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.2",
    "react-hot-loader": "next",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "ava": {
    "files": [
      "test/ava/**/*.test.js"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ]
  }
}
